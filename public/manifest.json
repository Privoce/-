{
  "name": "__MSG_extName__",
  "short_name": "Portal",
  "description": "__MSG_extDescription__",
  "version": "1.12.1",
  "manifest_version": 2,
  "homepage_url": "https://nicegoodthings.com/",
  "permissions": ["tabs", "identity", "history", "storage"],
  "content_security_policy": "script-src 'self'; object-src 'self'; img-src https: data: http: chrome:;",
  "browser_action": {
    "default_icon": {
      "16": "/favicon-16x16.png",
      "32": "/favicon-32x32.png",
      "48": "/android-chrome-192x192.png",
      "128": "/android-chrome-192x192.png"
    },
    "default_title": "Vera - Co-browsering"
  },
  "icons": {
    "16": "/favicon-16x16.png",
    "32": "/favicon-32x32.png",
    "48": "/android-chrome-192x192.png",
    "128": "/android-chrome-192x192.png"
  },
  "key": "",
  "oauth2": {
    "client_id": "1034738316090-7r656fifkotsla4cb8huu6eqdqshfa2q.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/calendar"]
  },
  "chrome_url_overrides": {
    "newtab": "/index.html"
  },
  "background": {
    "scripts": ["./crx/bg.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "exclude_matches": ["https://nicegoodthings.com/transfer/*"],
      "js": [
        "crx/vera/assets/authing.min.js",
        "crx/vera/assets/tf.min.js",
        "crx/vera/assets/body-pix@2.1.js",
        "crx/vera/assets/peerjs.min.js",
        "crx/vera/assets/plain.draggable.min.js",
        "crx/vera/assets/rangy-core.js",
        "crx/vera/assets/rangy-serializer.js",
        "crx/vera/assets/allow.copy.js",
        "crx/vera/content_script.js"
      ],
      "css": ["crx/vera/style.css"],
      "run_at": "document_idle"
    },
    {
      "matches": ["https://nicegoodthings.com/transfer/*"],
      "js": ["crx/vera/catch.invite.id.js"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": ["crx/vera/*"],
  "options_page": "./crx/option.html",
  "default_locale": "en"
}
