{
  "name": "Portal",
  "short_name": "Portal",
  "description": "像积木一样自由组建你的浏览器主页！",
  "version": "1.5.0",
  "manifest_version": 2,
  "homepage_url": "https://nicegoodthings.com/",
  "permissions": ["tabs", "identity"],
  "content_security_policy": "script-src 'self'; object-src 'self'; img-src https: data: http: chrome:;",
  "icons": {
    "16": "/favicon-16x16.png",
    "32": "/favicon-32x32.png",
    "48": "/android-chrome-192x192.png",
    "128": "/android-chrome-192x192.png"
  },
  "key": "",
  "oauth2": {
    "client_id": "1034738316090-tf816ulnekevogf577eron4d6iktdu01.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/calendar"]
  },
  "chrome_url_overrides": {
    "newtab": "/index.html"
  },
  "background": {
    "scripts": ["./crx/bg.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["crx/vemos/assets/peerjs.min.js", "crx/vemos/content_script.js"],
      "css": ["crx/vemos/style.css"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": ["crx/vemos/*"],
  "options_page": "./crx/option.html"
}
